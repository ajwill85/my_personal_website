# AWS Deployment Configuration
# Copy this file to .env and fill in your values
# NEVER commit the .env file to git

# S3 Bucket Name (from CloudFormation outputs)
VITE_S3_BUCKET_NAME=your-bucket-name-here

# CloudFront Distribution ID (from CloudFormation outputs)
VITE_CLOUDFRONT_DISTRIBUTION_ID=E1234ABCDEFGH

# AWS Profile (optional, defaults to 'default')
AWS_PROFILE=default

# AWS Region
AWS_REGION=us-east-1

# Visitor Counter API Endpoint (if using visitor counter)
VITE_VISITOR_COUNTER_API=https://your-api-id.execute-api.us-east-1.amazonaws.com/prod/count

# Contact Form API Endpoint (if using contact form)
VITE_CONTACT_FORM_API=https://your-api-id.execute-api.us-east-1.amazonaws.com/prod/contact
